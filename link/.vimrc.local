"function! P_Filetype()
"python << EOF
"import vim
"ft = vim.eval("&filetype")
"if ft == "python" or ft == "vim":
"   import re
"   data = '''
"           Conflict Analysis & Resolution
"           Conflict Analysis & Resolution
"           Conflict Analysis & Resolution
"           Conflict Analysis & Resolution
"   '''
"   match = re.search("(\w+)", data).group(1)
"   vim.command("r!cat <<< "+match)
"elif ft == "sh" or ft == 'python':
"   vim.command("echo"+vim.current.buffer.name)
"EOF
"endfun
